        <span class="cp">&lt;?php</span>
            <span class="p">}</span>
            <span class="nb">unset</span><span class="p">(</span><span class="nv">$_SESSION</span><span class="p">[</span><span class="s1">'hasil_create'</span><span class="p">]);</span>
        <span class="p">}</span>
        <span class="cp">?&gt;</span>
